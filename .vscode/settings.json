{
    "window.title": "${rootName}",
    "liveServer.settings.port": 5502,
    "scriptmonkey.activation condition.userscript header": true,
    "scriptmonkey.metaData.default.author": "moojor224",
    "scriptmonkey.metaData.default.namespace": "https://github.com/moojor224",
    "indentRainbow.includedLanguages": [
        "python"
    ],
    "commandOnAllFiles.includeFileExtensions": [
        ".js"
    ],
    "commandOnAllFiles.commands": {
        "organize imports": {
            "command": "editor.action.organizeImports",
            "description": "Organize imports",
            "includeFiles": [
                {
                    "regex": ".*\\.js$"
                }
            ]
        }
    }
}